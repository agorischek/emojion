{
  "name": "emj",
  "version": "0.0.1",
  "description": "Emoji Object Notation",
  "license": "MIT",
  "repository": "https://github.com/agorischek/emojion",
  "author": {
    "name": "Alex Gorischek",
    "email": "alex.gorischek@gmail.com",
    "url": ""
  },
  "keywords": [
    "object notation"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/src/index.js",
  "scripts": {
    "clean": "rimraf lib && rimraf coverage",
    "format": "prettier --write \"{src,test}/**/*.ts\" && prettier --write README.md",
    "lint": "eslint src/**/*.ts",
    "prebuild": "npm run clean && npm run format && npm run lint && echo Using TypeScript && tsc --version",
    "build": "npm run build:grammar && npm run build:lib && npm run build:bundle",
    "build:grammar": "mkdir -p gen && nearleyc src/grammar.ne -o gen/grammar.ts && replace-in-file \"Lexer: lexer,\" \"// @ts-ignore\nLexer: lexer,\" gen/grammar.ts",
    "build:bundle": "npm run rollup",
    "build:lib": "tsc --pretty",
    "start": "npm run build && npm run test",
    "test": "npm run build:grammar && jest",
    "coverage": "jest --coverage",
    "update": "ncu -u && npm i && npm audit fix",
    "rollup": "rollup --config",
    "watch": "npm run build -- --watch",
    "watch:test": "jest --watch"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "moo": "^0.5.1",
    "nearley": "^2.19.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/moo": "^0.5.0",
    "@types/nearley": "^2.11.0",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "acorn": "^7.1.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "npm-check-updates": "^3.2.2",
    "prettier": "^1.19.1",
    "replace-in-file": "^4.2.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.5",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.2",
    "tslib": "^1.10.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
